// test a complex hierarchy of nested functions

test1 = (a: str): str -> {
    println(a);
    return a;
};

test2 = (a: str, b: num): str, num -> {
    return (test1(a), b);
};

test3 = (a: str, b: num, c: str) -> {
    println(a);
    println(c);
};

test3(test2("bazinga2", 5.2), test1("bazinga"));